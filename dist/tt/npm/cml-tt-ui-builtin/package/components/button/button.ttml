<button class=" {{btnClasses}}  cml-base cml-button" style="{{mrBtnStyle}}" bindtap="_cmlEventProxy" bindtouchstart="_cmlEventProxy" bindtouchend="_cmlEventProxy" open-type="{{openType}}" lang="{{lang}}" session-from="{{sessionFrom}}" send-message-title="{{sendMessageTitle}}" send-message-path="{{sendMessagePath}}" send-message-img="{{sendMessageImg}}" show-message-card="{{showMessageCard}}" app-parameter="{{appParameter}}" bindgetuserinfo="_cmlEventProxy" bindcontact="_cmlEventProxy" bindgetphonenumber="_cmlEventProxy" binderror="_cmlEventProxy" bindopensetting="_cmlEventProxy" data-eventtap="{{['onclick']}}" data-eventtouchstart="{{['touchstart']}}" data-eventtouchend="{{['touchend']}}" data-eventgetuserinfo="{{['getuserinfo']}}" data-eventcontact="{{['contact']}}" data-eventgetphonenumber="{{['getphonenumber']}}" data-eventerror="{{['error']}}" data-eventopensetting="{{['opensetting']}}">
  <text class=" {{textClasses}}  cml-base cml-text" tt:if="{{text}}" style="{{mrTextStyle}}">{{text}}</text>
  <slot tt:else class=" cml-base cml-slot"></slot>
</button>