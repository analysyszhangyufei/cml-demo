<view class=" {{ disabled?'cml-switch-disabled':'cml-switch' }}  cml-base cml-view">
  <switch checked="{{ checked }}" disabled="{{ disabled }}" bindchange="_cmlEventProxy" color="{{ opencolor }}" data-eventchange="{{['switchChange']}}" class=" cml-base cml-switch" />
    <text class=" cml-switch-label  cml-base cml-text" tt:if="{{ label }}">{{label}}</text>
</view>