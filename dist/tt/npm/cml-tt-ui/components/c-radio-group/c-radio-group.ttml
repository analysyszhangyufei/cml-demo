<view class=" cml-radio-group  cml-base cml-view" style="{{horizontalStyle}}">
  <cml-buildin-radio tt:for="{{radioList}}" color="{{item.color || color}}" data-index="{{ index }}" data-uid="{{ item.uid }}" checked="{{ item.checked }}" label="{{ item.label }}" group-index="{{ index }}" position="{{ subPosition }}" disabled="{{ item.disabled ? true : false }}" bindchange="_cmlEventProxy" cstyle="{{item.style || itemStyle}}" data-eventchange="{{['valueChange']}}" class=" cml-base cml-cml-buildin-radio">
  </cml-buildin-radio>
</view>