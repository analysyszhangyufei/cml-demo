<cml-buildin-page title="refresh-view" class=" cml-base cml-cml-buildin-page">
  <cml-buildin-refresh-view enablePullUpLoad="{{true}}" pullingDown="{{pullingDown}}" pullingUp="{{pullingUp}}" bindonPullDownRefresh="_cmlEventProxy" bindonPullUpLoad="_cmlEventProxy" data-eventonpulldownrefresh="{{['pullDownRefreshHandle']}}" data-eventonpullupload="{{['pullUploadHandle']}}" class=" cml-base cml-cml-buildin-refresh-view">
      <view class=" content  cml-base cml-view">
        <c-header title="refresh-view" class=" cml-view cml-c-header"></c-header>
        <text class=" text-item  cml-base cml-text" tt:for="{{texts}}">{{item}}</text>
      </view>
      <text class=" loading-text  cml-base cml-text">{{loadingText}}</text>
  </cml-buildin-refresh-view>
</cml-buildin-page>