<cml-buildin-page title="image" class=" cml-base cml-cml-buildin-page">
    <view class=" container  cml-base cml-view">
        <c-header title="image" desc="这是一张本地图片" class=" cml-view cml-c-header"></c-header>
        <image src="{{imageSrc}}" style="width:682rpx;height:600rpx" bindload="_cmlEventProxy" binderror="_cmlEventProxy" data-eventload="{{['imageLoad']}}" data-eventerror="{{['imageError']}}" class=" cml-base cml-image"></image>
    </view>
</cml-buildin-page>