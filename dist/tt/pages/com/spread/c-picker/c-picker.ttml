<cml-buildin-page title="c-picker演示" class=" cml-base cml-cml-buildin-page">
    <c-header title="c-picker" class=" cml-view cml-c-header"></c-header>
    <view class=" cml-base cml-view">
        <text class=" select-text  cml-base cml-text" bindtap="_cmlEventProxy" data-eventtap="{{['showClick']}}">点击选择：{{provins[provinsIndex]}}</text>
        <c-picker show="{{pickerShow}}" height="{{500}}" header-height="{{100}}" text-align="center" data-scroller-height="{{400}}" list="{{provins}}" default-index="{{defaultIndex}}" confirm-btn-style="color:red" cancel-btn-style="color:red" bindcancel="_cmlEventProxy" bindconfirm="_cmlEventProxy" bindselectchange="_cmlEventProxy" data-eventcancel="{{['cancel']}}" data-eventconfirm="{{['confirm']}}" data-eventselectchange="{{['selectchange']}}" class=" cml-view cml-c-picker">
        </c-picker>
    </view>
</cml-buildin-page>