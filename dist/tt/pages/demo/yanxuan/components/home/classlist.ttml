<view class=" classList  cml-base cml-view">
  <view class=" classList-item  cml-base cml-view" tt:for="{{classifyImgList}}" tt:for-index="i" tt:for-item="item" bindtap="_cmlEventProxy" data-idx="{{i}}" data-eventtap="{{['change']}}">
    <image class=" classList-item-img  cml-base cml-image" src="{{item.imgUrl}}" style="width:110rpx;height:110rpx"></image>
    <view class=" classList-item-title  cml-base cml-view">
      <text class=" cml-base cml-text">{{item.title}}</text>
    </view>
  </view>
</view>