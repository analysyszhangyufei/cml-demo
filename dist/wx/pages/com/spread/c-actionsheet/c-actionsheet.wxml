<cml-buildin-page title="c-actionsheet演示" class=" cml-base cml-page">
  <c-header title="c-actionsheet" class=" cml-view cml-c-header"></c-header>
  <cml-buildin-button class="btn  cml-base cml-button" bindonclick="_cmlEventProxy" data-eventonclick="{{['showActionSheet']}}" text="ActionSheet"></cml-buildin-button>
  <cml-buildin-button class="btn  cml-base cml-button" bindonclick="_cmlEventProxy" data-eventonclick="{{['showActionSheetActive']}}" text="ActionSheet - active"></cml-buildin-button>
  <cml-buildin-button class="btn  cml-base cml-button" bindonclick="_cmlEventProxy" data-eventonclick="{{['showActionSheetPicker']}}" text="ActionSheet - pickerStyle"></cml-buildin-button>
  <c-actionsheet show="{{actionShow}}" title="{{title}}" list="{{list}}" active="{{active}}" cancel-txt="取消" picker-style="{{pickerStyle}}" bindcancel="_cmlEventProxy" data-eventcancel="{{['cancel']}}" bindselect="_cmlEventProxy" data-eventselect="{{['select']}}" class=" cml-view cml-c-actionsheet"></c-actionsheet>
</cml-buildin-page>