.title-text {
  color: #999;
  margin: 30rpx 20rpx 10rpx;
  display: block;
  font-size: 28rpx;
}
.picker-item {
  background: #fff;
  border-top: 1px solid #d9d9d9;
  border-bottom: 1px solid #d9d9d9;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
}
.picker-text-left {
  font-size: 40rpx;
  height: 70rpx;
  line-height: 70rpx;
  margin-left: 20rpx;
  width: 300rpx;
}
.picker-text-right {
  font-size: 40rpx;
  height: 70rpx;
  line-height: 70rpx;
  margin-left: 20rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
}
.select-text {
  font-size: 32rpx;
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
