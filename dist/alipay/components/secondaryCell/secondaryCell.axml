<view class="secondary-cell  cml-base cml-view">
  <text class="cell-title  cml-base cml-text">{{title}}</text>
  <view class="cell-divider  cml-base cml-view"></view>
  <view class="btn-group  cml-base cml-view" a:for="{{list}}" a:for-item="cellInfo">
    <text class="btn-tip  cml-base cml-text">{{cellInfo.title}}</text>
    <view class="action-btn  cml-base cml-view"><cml-buildin-button class="action-btn  cml-base cml-button" size="none" type="blue" text="{{cellInfo.btnText}}" btn-style="height:100cpx;width:658cpx;margin: 24cpx 0 30cpx;" data-index="{{index}}" onOnclick="_cmlInlineStatementEventProxy" data-arg0="{{index}}" data-args="index" data-eventonclick="onBtnClick" onCmlPropsEventProxy="_cmlPropsEventProxy"></cml-buildin-button></view>
  </view>
</view>