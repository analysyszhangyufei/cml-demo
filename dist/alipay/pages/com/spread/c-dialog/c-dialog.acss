.container {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  background: #f8f8f8;
}
.btn {
  margin-top: 20rpx;
  -webkit-align-self: center;
          align-self: center;
}
.custom-header {
  font-size: 32rpx;
  padding: 30rpx 20rpx;
}
.custom-content {
  padding: 60rpx;
  font-size: 28rpx;
  text-align: center;
  color: #666;
}
.custom-footer {
  text-align: center;
  border-top: 0.5rpx solid #f0f0f0;
  padding: 30rpx 0;
  font-size: 24rpx;
  font-size: 32rpx;
  color: #fc9153;
}
