.row {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.page-home {
  background-color: #F0F2F7;
}
.logo-row {
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.logo-desc {
  color: #A2ACC1;
  font-size: 50rpx;
  margin-bottom: 100rpx;
}
.logo-icon {
  width: 210rpx;
  height: 210rpx;
  margin-top: 100rpx;
  margin-bottom: 37rpx;
}
.list {
  padding: 0 46rpx;
  margin-bottom: 30rpx;
}
.content-item {
  height: 280rpx;
  background: #FFFFFF;
  box-shadow: 0 2rpx 16rpx 0 rgba(146, 167, 215, 0.12);
  border-radius: 4rpx;
  background-color: white;
  margin-bottom: 30rpx;
}
.content-item-left-icon {
  width: 100rpx;
  height: 100rpx;
  margin-left: 40rpx;
}
.text-group {
  padding: 0 55rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.text-title {
  font-size: 32rpx;
  color: #172742;
  letter-spacing: 0;
  text-align: left;
  margin-bottom: 5rpx;
}
.text-desc {
  font-size: 24rpx;
  color: #A2ACC1;
  letter-spacing: 0;
  height: 66rpx;
  line-height: 33rpx;
  text-align: left;
}
.content-item-right-icon {
  width: 35rpx;
  height: 35rpx;
  margin-right: 44rpx;
}
