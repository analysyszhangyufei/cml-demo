<view class="cml-radio-group  cml-base cml-view" style="{{horizontalStyle}}">
  <cml-buildin-radio a:for="{{radioList}}" data-index="{{ index }}" data-uid="{{ item.uid }}" checked="{{ item.checked }}" label="{{ item.label }}" group-index="{{ index }}" position="{{ subPosition }}" disabled="{{ item.disabled ? true : false }}" onChange="_cmlEventProxy" data-eventchange="valueChange" class=" cml-base cml-radio" onCmlPropsEventProxy="_cmlPropsEventProxy">
  </cml-buildin-radio>
</view>