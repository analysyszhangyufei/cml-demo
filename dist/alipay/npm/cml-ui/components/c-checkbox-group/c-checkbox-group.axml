<view class="cml-checkbox-group  cml-base cml-view cml-2805ad3e" style="{{horizontalStyle}}">
  <cml-buildin-checkbox a:for="{{ option }}" data-index="{{ index }}" data-uid="{{ item.uid }}" checked="{{ item.checked }}" label="{{ item.label }}" group-index="{{ index }}" position="{{ subPosition }}" disabled="{{ item.disabled ? true : false }}" uncheck-icon="{{ uncheckIcon }}" checked-icon="{{ checkedIcon }}" cstyle="{{item.style || itemStyle}}" onChange="_cmlEventProxy" data-eventchange="{{['valueChange']}}" class=" cml-base cml-checkbox cml-2805ad3e">
  </cml-buildin-checkbox>
</view>