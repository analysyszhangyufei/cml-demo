<picker-view indicator-style="height: 72rpx;" style="height: {{height}}rpx;" value="{{value}}" onChange="bindChange" class=" cml-base cml-origin-picker-view">
  <picker-view-column class=" cml-base cml-origin-picker-view-column">
    <view a:for="{{list}}" style="line-height:72rpx;align-items:center" class=" cml-base cml-view"><text class=" cml-base cml-text">{{item}}</text></view>
  </picker-view-column>
</picker-view>