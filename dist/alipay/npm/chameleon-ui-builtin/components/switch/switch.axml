<view class="{{ disabled?'cml-switch-disabled':'cml-switch' }}  cml-base cml-view cml-bb1baf7c">
  <switch checked="{{ checked }}" disabled="{{ disabled }}" onChange="_cmlEventProxy" data-eventchange="switchChange" color="{{ opencolor }}" class=" cml-base cml-switch cml-bb1baf7c" />
    <text class="cml-switch-label  cml-base cml-text cml-bb1baf7c" a:if="{{ label }}">{{label}}</text>
</view>