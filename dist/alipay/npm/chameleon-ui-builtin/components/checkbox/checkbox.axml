<view class="cml-checkbox  cml-base cml-view cml-5c1fce2a">
  <view class="cml-checkbox-wrap  cml-base cml-view cml-5c1fce2a" onTap="_cmlEventProxy" data-eventtap="changeCheck">
    <view class="cml-checkbox-check  cml-base cml-view cml-5c1fce2a" style="{{checkStyle}}">
      <view class="{{ checkClass }}  cml-base cml-view cml-5c1fce2a">
        <image class="inner-icon  cml-base cml-image cml-5c1fce2a" src="{{ checkImg }}" />
      </view>
    </view>
    <text class="cml-checkbox-label  cml-base cml-text cml-5c1fce2a">{{label}}</text>
  </view>
</view>